{
  "name": "frac-log",
  "version": "1.0.0",
  "description": "a colorful template literal",
  "main": "dist/index.js",
  "module": "dist/index.mjs",
  "types": "dist/index.d.ts",
  "exports": {
    ".": {
      "require": "./dist/index.js",
      "import": "./dist/index.mjs"
    }
  },
  "scripts": {
    "dev": "run-p dev:*",
    "dev:ts": "tsup-node --watch src",
    "dev:nodemon": "nodemon dev/index.js",
    "build": "tsup-node",
    "docs": "typedoc src/index.ts --theme minimal"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/fractalhq/log.git"
  },
  "keywords": [
    "logger"
  ],
  "author": "Fractal",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/fractalhq/log/issues"
  },
  "homepage": "https://github.com/fractalhq/log#readme",
  "devDependencies": {
    "@types/node": "^16.7.13",
    "nodemon": "^2.0.12",
    "npm-run-all": "^4.1.5",
    "tsup": "^4.14.0",
    "typedoc": "^0.21.9",
    "typescript": "^4.4.2"
  }
}